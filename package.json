{
  "name": "d2-flux",
  "version": "0.1.2",
  "description": "Flux implementation and utilities",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register --require test/config/setup.js --recursive test",
    "test-watch": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register --require test/config/setup.js --watch --recursive test",
    "lint": "./node_modules/eslint/bin/eslint.js src && ./node_modules/eslint/bin/eslint.js --env node,mocha --global expect,spy,stub --rule 'no-unused-expressions: 0' test",
    "coverage": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --require test/config/setup.js --recursive test",
    "build": "./node_modules/babel/bin/babel/index.js src --out-dir lib && cp package.json ./lib/package.json",
    "publish": "cd lib && npm publish"
  },
  "author": "Mark Polak",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-eslint": "^3.1.23",
    "chai": "^3.0.0",
    "coveralls": "^2.11.2",
    "eslint": "^0.24.0",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "flux": "^2.0.3",
    "loglevel": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Markionium/d2-flux.git"
  },
  "bugs": {
    "url": "https://github.com/Markionium/d2-flux/issues"
  },
  "homepage": "https://github.com/Markionium/d2-flux#readme"
}
